
// 

var util = require('util');

var Chart = require('./chart');

module.exports = (function() {
  var Line = function() {
    this.initialize();
  };

  util.inherits(Line, Chart);

  Line.prototype.initialize = function() {
    Line.super_.prototype.initialize.call(this);

    this.type = 'pie';
    this.setAutoScaling();
  };

  return Line;
}());